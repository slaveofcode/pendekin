{
  "name": "pendekin",
  "version": "1.0.0",
  "description": "Simple self-hosted shorthener url app that produce short url with primary service based on RESTful API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:debug": "node --inspect=1891 server.js",
    "db:migrate": "sequelize db:migrate --config=./config/config.json",
    "db:migrate:undo": "sequelize db:migrate:undo --config=./config/config.json",
    "db:migration:create:info": "sequelize help:migration:create",
    "db:migration:create": "sequelize migration:create --name",
    "db:seed:all": "sequelize db:seed:all --config=./config/config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slaveofcode/pendekin.git"
  },
  "keywords": [
    "shortener",
    "url-shortener",
    "link-shortener",
    "self-hosted",
    "shortener-rest",
    "shortener-api",
    "api-rest",
    "restful",
    "restful-api"
  ],
  "author": "Aditya Kresna Permana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slaveofcode/pendekin/issues"
  },
  "homepage": "https://github.com/slaveofcode/pendekin#readme",
  "dependencies": {
    "bunyan-winston-adapter": "^0.2.0",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "randomstring": "^1.1.5",
    "restify": "^5.2.0",
    "restify-cors-middleware": "^1.0.1",
    "restify-routing": "^0.3.11",
    "sequelize": "^4.7.5",
    "sequelize-cli": "^2.8.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  }
}
